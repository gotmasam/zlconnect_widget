tion 1s linear 0s infinite;\n}\n\n@keyframes ball-animation {\n    0% {\n        width: 20px;\n        height: 20px;\n        opacity: 1;\n    }\n\n    100% {\n        width: 6px;\n        height: 6px;\n        opacity: .2;\n        margin-left: 7px;\n    }\n}\n\n</style>\n"]}]}